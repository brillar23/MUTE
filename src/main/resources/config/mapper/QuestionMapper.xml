<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.music.mute.mapper.QuestionMapper">

 <select id="getQuestionByNumber" parameterType="java.lang.Integer" resultType="com.music.mute.test.QuestionVO">
        SELECT * FROM QUESTION WHERE QNO = #{QNO}
    </select>
    
    <select id="getChoiceByNumber" parameterType="java.lang.Integer" resultType="com.music.mute.test.ChoiceVO">
        SELECT * FROM CHOICE WHERE CNO = #{CNO}
    </select>
	
</mapper>